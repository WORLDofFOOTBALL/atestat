{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "compat-api/css": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/forms": "off",
    "highest-available-document-mode": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "css-prefix-order": "off",
    "disown-opener": "off",
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 106",
    "not chrome <= 106",
    "not edge <= 106",
    "not firefox <= 105",
    "not safari <= 16",
    "not samsung <= 18"
  ]
}